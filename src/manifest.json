{
  "name": "Sketch Auto-Spec",
  "description": "Plugins to label symbols",
  "author": "LinkedIn Labs",
  "homepage": "https://github.com/linkedinlabs/sketch-auto-spec",
  "version": "1.0-b",
  "identifier": "com.linkedinlabs.sketch.auto-spec-plugin",
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "commands": [
    {
      "name": "Hello World",
      "identifier": "hello-world",
      "shortcut": "ctrl shift h",
      "script": "./Core.js",
      "handler": "helloWorld"
    },
    {
      "name": "What Am I?",
      "identifier": "what-am-i",
      "shortcut": "ctrl shift i",
      "script": "./Core.js",
      "handler": "whatAmI"
    },
    {
      "name" : "Selection Change Listener",
      "identifier": "selection-change-listener",
      "script": "./Core.js",
      "handlers" : {
        "actions": {
          "SelectionChanged.finish": "onSelectionChange"
        }
      }
    },
    {
      "name" : "Open Document Listener",
      "identifier": "open-document-listener",
      "script": "./Core.js",
      "handlers" : {
        "actions": {
          "OpenDocument": "onOpenDocument"
        }
      }
    },
    {
      "name": "View GUI",
      "identifier": "view-gui",
      "script": "./GUI.js",
      "handlers": {
        "run": "onRun",
        "actions": {
          "Shutdown": "onShutdown"
        }
      }
    }
  ],
  "menu": {
    "title": "Auto-Spec",
    "items": [
      "hello-world",
      "what-am-i",
      "view-gui"
    ]
  }
}
